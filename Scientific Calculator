    private void b28ActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
         double t=Double.parseDouble(TextField1.getText());
      double fact=1;
      
      while(t!=0)
      {
          fact=fact*t;
          t--;
      }
          TextField1.setText("");
        TextField1.setText(TextField1.getText() + fact);
           
        
    }                                   

    private void b29ActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
        Double a=Double.parseDouble(String.valueOf(TextField1.getText()));
        a=a*(-1);
        TextField1.setText(String.valueOf(a));
        
        
    }                                   

